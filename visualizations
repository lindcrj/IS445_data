<!DOCTYPE html>
<html>
<head>
  <title>Vega-Lite Visualization</title>
</head>
<body>
  <div id="vis"></div>

  <script src="https://cdn.jsdelivr.net/npm/vega@5"></script>
  <script src="https://cdn.jsdelivr.net/npm/vega-lite@5"></script>
  <script src="https://cdn.jsdelivr.net/npm/vega-embed@6"></script>
  <script src="https://raw.githubusercontent.com/lindcrj/IS445_data/main/vegalite_visualization.json"></script>

  <!-- Embed the visualization -->
  <script>
    // Assuming your-vega-lite-spec.json contains your Vega-Lite specification
    const vegaLiteSpec = require('./https://raw.githubusercontent.com/lindcrj/IS445_data/main/vegalite_visualization.json');
  
    // Assuming 'vis' is the ID of the div where you want to render the visualization
    const div = document.getElementById('vis');
  
    // Embed the visualization in the 'vis' div
    vegaEmbed(div, vegaLiteSpec);
  </script>
</body>
</html>

